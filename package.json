{
  "name": "@human-writes/core",
  "packageManager": "yarn@3.5.1",
  "type": "module",
  "version": "0.5.53",
  "description": "Write HTML and code snippet like a human",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "src/*"
  ],
  "scripts": {
    "dev": "webpack --config webpack.config.js",
    "build": "gulp -f gulpfile.js",
    "lint": "npx eslint --ext .mjs,.cjs,.js ./src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:human-writes/core.git"
  },
  "bugs": {
    "url": "https://github.com/human-writes/core/issues"
  },
  "homepage": "https://github.com/human-writes/core#readme",
  "types": "index.d.ts",
  "typings": "./typings, ./node_modules",
  "module": "@human-writes/core.d.ts",
  "main": "index.js",
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "HTML",
    "code-snippet",
    "human",
    "writing"
  ],
  "author": "David Blanchard",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "babel-loader": "^9.1.2",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-eslint": "^7.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsdoc": "^45.0.0",
    "eslint-plugin-node": "^11.1.0",
    "glob-parent": "^6.0.2",
    "gulp": "^4.0.2",
    "gulp-clean-css": "4.3.0",
    "gulp-cli": "^2.3.0",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1",
    "webpack-stream": "^7.0.0"
  }
}
